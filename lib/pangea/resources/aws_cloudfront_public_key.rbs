# RBS type definitions for AWS CloudFront Public Key resource

module Pangea
  module Resources
    module AWS
      module Types
        class CloudFrontPublicKeyAttributes < Dry::Struct
          def self.new: (Hash[Symbol, untyped] attributes) -> instance

          def name: () -> String
          def encoded_key: () -> String
          def comment?: () -> String?

          def key_type: () -> String
          def key_size: () -> String
          def security_level: () -> String
          def strong_key?: () -> bool
        end
      end

      # Create an AWS CloudFront Public Key
      def aws_cloudfront_public_key: (
        Symbol name,
        name: String,
        encoded_key: String,
        ?comment: String
      ) -> ResourceReference
    end
  end
end