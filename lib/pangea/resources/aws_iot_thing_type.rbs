      class IotThingTypeAttributes < Dry::Struct
        attr_reader thing_type_name: String
        attr_reader thing_type_properties: Hash[Symbol, untyped]?
        attr_reader tags: Hash[Symbol, String]

        def self.new: (Hash[Symbol, untyped] attributes) -> IotThingTypeAttributes

        def has_description?: () -> bool
