        def security_recommendations: () -> Array[String]
        def example_thing_configuration: () -> Hash[Symbol, untyped]
        def required_permissions: () -> Array[String]
        def cost_impact_analysis: () -> Hash[Symbol, untyped]
        def compatibility_check: (String other_type_name) -> Hash[Symbol, untyped]
