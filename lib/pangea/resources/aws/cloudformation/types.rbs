# AWS CloudFormation Extended Resource Types
# Type definitions for advanced CloudFormation resources including
# stack sets, type registration, and publishing capabilities.

module Pangea
  module Resources
    module AWS
      module CloudFormation
        module Types
          # CloudFormation Stack Set attributes
          class CloudFormationStackSetAttributes < Dry::Struct
            def self.new: (untyped attributes) -> CloudFormationStackSetAttributes
            
            def organization_managed?: () -> bool
            def has_auto_deployment?: () -> bool
            def has_managed_execution?: () -> bool
            def template_source: () -> Symbol
            def requires_capabilities?: () -> bool
            def to_h: () -> Hash[Symbol, untyped]
            
            attr_reader name: String
            attr_reader template_body: String?
            attr_reader template_url: String?
            attr_reader parameters: Hash[untyped, untyped]
            attr_reader capabilities: Array[String]
            attr_reader description: String?
            attr_reader execution_role_name: String?
            attr_reader administration_role_arn: String?
            attr_reader permission_model: String?
            attr_reader call_as: String?
            attr_reader auto_deployment: Hash[untyped, untyped]?
            attr_reader managed_execution: Hash[untyped, untyped]?
            attr_reader operation_preferences: Hash[untyped, untyped]?
            attr_reader tags: Hash[untyped, untyped]
          end
          
          # CloudFormation Stack Set Instance attributes
          class CloudFormationStackSetInstanceAttributes < Dry::Struct
            def self.new: (untyped attributes) -> CloudFormationStackSetInstanceAttributes
            
            def organization_deployment?: () -> bool
            def account_deployment?: () -> bool
            def to_h: () -> Hash[Symbol, untyped]
            
            attr_reader stack_set_name: String
            attr_reader account_id: String?
            attr_reader region: String?
            attr_reader deployment_targets: Hash[untyped, untyped]?
            attr_reader parameter_overrides: Hash[untyped, untyped]
            attr_reader retain_stack: bool
            attr_reader operation_preferences: Hash[untyped, untyped]?
            attr_reader call_as: String?
          end
          
          # CloudFormation Stack Instances attributes  
          class CloudFormationStackInstancesAttributes < Dry::Struct
            def self.new: (untyped attributes) -> CloudFormationStackInstancesAttributes
            
            def deployment_scope: () -> String
            def multi_region?: () -> bool
            def to_h: () -> Hash[Symbol, untyped]
            
            attr_reader stack_set_name: String
            attr_reader deployment_targets: Hash[untyped, untyped]?
            attr_reader regions: Array[String]
            attr_reader parameter_overrides: Hash[untyped, untyped]
            attr_reader operation_preferences: Hash[untyped, untyped]?
            attr_reader call_as: String?
            attr_reader operation_id: String?
          end
          
          # CloudFormation Type attributes
          class CloudFormationTypeAttributes < Dry::Struct
            def self.new: (untyped attributes) -> CloudFormationTypeAttributes
            
            def resource_type?: () -> bool
            def hook_type?: () -> bool
            def has_logging?: () -> bool
            def to_h: () -> Hash[Symbol, untyped]
            
            attr_reader type: String
            attr_reader type_name: String
            attr_reader schema: String?
            attr_reader schema_handler_package: String?
            attr_reader source_url: String?
            attr_reader documentation_url: String?
            attr_reader execution_role_arn: String?
            attr_reader logging_config: Hash[untyped, untyped]?
            attr_reader client_request_token: String?
          end
        end
        
        # Stack Set resource module
        module StackSet
          def aws_cloudformation_stack_set: (Symbol name, Hash[Symbol, untyped] attributes) -> ResourceReference
        end
        
        # Stack Set Instance resource module  
        module StackSetInstance
          def aws_cloudformation_stack_set_instance: (Symbol name, Hash[Symbol, untyped] attributes) -> ResourceReference
        end
        
        # Stack Instances resource module
        module StackInstances
          def aws_cloudformation_stack_instances: (Symbol name, Hash[Symbol, untyped] attributes) -> ResourceReference
        end
        
        # CloudFormation Type resource module
        module Type
          def aws_cloudformation_type: (Symbol name, Hash[Symbol, untyped] attributes) -> ResourceReference
        end
        
        # Type Activation resource module
        module TypeActivation
          def aws_cloudformation_type_activation: (Symbol name, Hash[Symbol, untyped] attributes) -> ResourceReference
        end
        
        # Publisher resource module
        module Publisher
          def aws_cloudformation_publisher: (Symbol name, Hash[Symbol, untyped] attributes) -> ResourceReference
        end
      end
    end
  end
end