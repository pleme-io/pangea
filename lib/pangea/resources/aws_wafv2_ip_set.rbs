# RBS type definitions for AWS WAF v2 IP Set resource

module Pangea
  module Resources
    module AWS
      module Types
        class WafV2IpSetAttributes < Dry::Struct
          def self.new: (Hash[Symbol, untyped] attributes) -> instance

          def name: () -> String
          def scope: () -> String
          def ip_address_version: () -> String
          def description?: () -> String?
          def addresses: () -> Array[String]
          def tags: () -> Hash[Symbol, String]

          def address_count: () -> Integer
          def has_cidr_blocks?: () -> bool
          def has_individual_ips?: () -> bool
          def estimated_ip_count: () -> Integer
        end
      end

      # Create an AWS WAF v2 IP Set
      def aws_wafv2_ip_set: (
        Symbol name,
        ?name: String,
        ?scope: Types::wafv2_scope,
        ?ip_address_version: Types::wafv2_ip_address_version,
        ?description: String,
        ?addresses: Array[String],
        ?tags: Hash[Symbol, String]
      ) -> ResourceReference
    end
  end
end